<div class="container">
  <h2>Refugee status</h2>
<div style='width: 800px;'>
  <div id="multi_overlays" style='width: 800px; height: 400px;'></div>
</div>
<div>
<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>

<script>
var handler = Gmaps.build('Google');
handler.buildMap({ internal: {id: 'multi_overlays'}}, function(){


  var polylines = handler.addPolylines(
    [
      [
        {lat: 42, lng: -6},
        {lat: 50, lng: -6}
      ]
    ],
    { strokeColor: '#FF0000'}
  );

  var polylines2 = handler.addPolylines(
    [
      [
        {lat: 30, lng: -20},
        {lat: 55, lng: -20}
      ]
    ],
    { strokeColor: '#FF0000'}
  );

  handler.bounds.extendWith(polylines);
  handler.bounds.extendWith(polylines2);
  handler.fitMapToBounds();
});
</script>